{
  "name": "stylelint-config-cssplus",
  "version": "1.0.1",
  "description": "A stylelint config for working with cssplus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thoughtbit/stylelint-config-cssplus.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "properties-order.json"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "cssplus"
  ],
  "authors": [
    "moocss <moocss@gmail.com> (https://github.com/moocss)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thoughtbit/stylelint-config-cssplus/issues"
  },
  "homepage": "https://github.com/thoughtbit/stylelint-config-cssplus#readme",
  "dependencies": {
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0"
  },
  "devDependencies": {
    "ci-publish": "^1.3.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "standard-version": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "release:ci": "standard-version && git push --follow-tags origin master && ci-publish",
    "pub": "npm publish && git push origin"
  }
}
